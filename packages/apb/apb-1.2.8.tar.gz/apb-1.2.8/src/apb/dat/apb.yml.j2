version: 1.0
name: {{ apb_dict['name'] }}
description: {{ apb_dict['description'] }}
bindable: {{ apb_dict['bindable'] }}
async: {{ apb_dict['async'] or 'optional' }}
metadata:
  displayName: {{ apb_dict['app_name'] }}
  dependencies: {{ apb_dict['dependencies'] or '[]' }}
plans:
  - name: default
    description: This default plan deploys {{ apb_dict['name'] }}
    free: True
    metadata: {}
    parameters: []
