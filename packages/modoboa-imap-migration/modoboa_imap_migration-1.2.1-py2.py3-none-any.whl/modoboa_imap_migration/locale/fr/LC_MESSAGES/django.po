#: auth_backends.py:37
#, python-format
msgid "Connection to IMAP server failed: %s"
msgstr "Echec lors de la connexion au serveur IMAP : %s"

#: forms.py:15
msgid "Connection settings"
msgstr "Paramètres de connexion"

#: forms.py:18
msgid "Server address"
msgstr "Adresse du serveur"

#: forms.py:20
msgid "Address of your IMAP server"
msgstr "Adresse de votre serveur IMAP"

#: forms.py:24
msgid "Use a secured connection"
msgstr "Utiliser une connexion sécurisée"

#: forms.py:26
msgid "Use a secured connection to access IMAP server"
msgstr "Utiliser une connexion sécurisée pour accéder au serveur IMAP"

#: forms.py:30
msgid "Server port"
msgstr "Port du serveur"

#: forms.py:32
msgid "Listening port of your IMAP server"
msgstr "Port d'écoute de votre serveur IMAP"

#: forms.py:35
msgid "OfflineIMAP settings"
msgstr "Paramètres OfflineIMAP"

#: forms.py:38
msgid "Concurrent sync jobs"
msgstr "Tâche(s) de synchronisation concurrente(s)"

#: forms.py:40
msgid "The maximum number of concurrent synchronization jobs"
msgstr "Nombre maximal de tâche de synchronisation concurrente"

#: forms.py:43
msgid "OfflineIMAP Filter settings"
msgstr "Paramètres de filtragre OfflineIMAP"

#: forms.py:46
msgid "Create Folders"
msgstr "Création des dossiers"

#: forms.py:48
msgid "Allow Creation of missing folders during sync"
msgstr "Autoriser la création des dossiers manquant pendant la synchro"

#: forms.py:53
msgid "Folder Filter Exclusions"
msgstr "Filtre d'exclusion de dossier"

#: forms.py:55
msgid ""
"Use a regular expression to explicitly include folders in sync. Example: "
"^Trash$|Del"
msgstr "Utiliser une expression régulière pour explicitement inclure des dossiers à synchroniser. Exemple : ^Trash$|Del"

#: forms.py:60
msgid "Folder Filter Inclusions"
msgstr "Filtre d'inclusion de dossier"

#: forms.py:62
msgid ""
"A comma seperated list of folders to explicitly include in sync even if "
"filtered by the Folder Filter Exclusions. Example: debian.user, "
"debian.personal "
msgstr "Une liste de dossiers séparés par des virgules à explicitement inclure pendant la synchronisation même s'ils sont concernés par le filtre d'exclusion. Exemple debian.user, debian.personal"

#: handlers.py:24
msgid "Cancel this account's migration"
msgstr "Annuler la migration de ce compte"

#: handlers.py:26
#, python-format
msgid "Cancel the migration for %s?"
msgstr "Annuler la migration de %s ?"

#: modo_extension.py:16
msgid "IMAP migration using OfflineIMAP"
msgstr "Migration IMAP basée sur OfflineIMAP"

#: modo_extension.py:19
msgid "Migrate existing mailboxes using IMAP and OfflineIMAP"
msgstr "Migrer des boites existantes en utilisant IMAP et OfflineIMAP"

#: modo_extension.py:25
msgid "IMAP migration"
msgstr "Migration IMAP"

#: views.py:22
msgid "Unknow migration"
msgstr "Migration inconnue"

#: views.py:26
msgid "Migration cancelled successfully"
msgstr "Migration annulée avec succès"
