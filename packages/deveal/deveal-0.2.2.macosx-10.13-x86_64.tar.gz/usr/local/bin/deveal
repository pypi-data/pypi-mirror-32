#!/usr/local/opt/python/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'deveal==0.2.2','console_scripts','deveal'
__requires__ = 'deveal==0.2.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('deveal==0.2.2', 'console_scripts', 'deveal')()
    )
