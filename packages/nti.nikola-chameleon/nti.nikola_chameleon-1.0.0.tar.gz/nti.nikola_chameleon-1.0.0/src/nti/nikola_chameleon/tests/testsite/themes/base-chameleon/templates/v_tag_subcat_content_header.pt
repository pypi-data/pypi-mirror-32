<tal:block tal:condition="options/subcategories|nothing"
           xmlns:tal="http://xml.zope.org/namespaces/tal"
           xmlns:i18n="http://xml.zope.org/namespaces/i18n">
	<span i18n:translate="">Subcategories:</span>
    <ul>
		<li tal:repeat="(tag,href) options/subcategories">
			<a href="${href}">${tag}</a>
		</li>
    </ul>
</tal:block>
