digraph {
	fake [style=invisible]
	0 [root=true shape=doublecircle]
	1
	fake -> 0 [style=bold]
	0 -> 1 [label="{A, B}"]
	0 -> 1 [label="{A}"]
	0 -> 1 [label="{A, C}"]
	0 -> 1 [label="{A, C, B}"]
	0 -> 0 [label="{C, B}"]
	0 -> 0 [label="{}"]
	0 -> 0 [label="{B}"]
	0 -> 0 [label="{C}"]
	1 -> 1 [label="{A, C, B}"]
	1 -> 0 [label="{B}"]
	1 -> 0 [label="{C, B}"]
	1 -> 1 [label="{A, B}"]
	label="always implication and next
[(true)*](([A](ff) | <true>((<B>(tt) & <true>(tt))) | [true](ff)))"
	fontsize=20
}
