	fake [style=invisible]
	0 [root=true shape=doublecircle]
	1 [shape=doublecircle]
	fake -> 0 [style=bold]
	0 -> 1 [label="{C}"]
	0 -> 0 [label="{A}"]
	0 -> 1 [label="{}"]
	0 -> 0 [label="{A, C}"]
	1 -> 1 [label="{A, B}"]
	1 -> 1 [label="{A}"]
	1 -> 1 [label="{A, C}"]
	1 -> 1 [label="{A, C, B}"]
	1 -> 1 [label="{C, B}"]
	1 -> 1 [label="{}"]
	1 -> 1 [label="{B}"]
	1 -> 1 [label="{C}"]
	label="release
[(((<A>(tt))? ; true))*](([true](ff) | [B](ff)))"
