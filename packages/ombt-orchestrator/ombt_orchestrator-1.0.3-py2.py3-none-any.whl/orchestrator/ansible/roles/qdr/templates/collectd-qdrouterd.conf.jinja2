# File generated by Ansible
<LoadPlugin python>
  Globals true
</LoadPlugin>

<Plugin python>
  LogTraces true
  Interactive false
  ModulePath "/opt/collectd-qdrouterd"
  Import "collectd_qdrouterd.collectd_plugin"
  <Module "collectd_qdrouterd.collectd_plugin">
{% for listener in item.listeners %}
{% if listener.role != "inter-router" %}
    Host "{{ item.router_id }}"
    Port: "{{ listener.port }}"
{% endif %}
{% endfor %}
    Username "guest"
    Password "guest"
    Router true
    Links true
    Addresses true
    Memory false
  </Module>
</Plugin>
