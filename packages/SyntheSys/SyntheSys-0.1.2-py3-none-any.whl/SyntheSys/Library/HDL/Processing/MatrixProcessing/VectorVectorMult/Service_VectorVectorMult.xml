<?xml version="1.0"?>
<service name="VectorVectorMult" type="simple" version="1.0" category="DSP">
  
  <input  name="VectorInput1" size="64" type="logic"/>
  <input  name="VectorInput2" size="64" type="logic"/>
  <output name="ScalarOutput" size="64" type="logic"/>
  
  <interface name="BasicBlock" direction="INOUT" number="1" pipeline="9">
    <data name="VectorInput1" serialization="9"/>
    <data name="VectorInput2" serialization="9"/>
    <data name="ScalarOutput" serialization="1"/>
    <protocol label="SetStimuli" position="0" signals="VectorInput1, VectorInput2"/>
    <protocol label="GetTrace" position="1" signals="ScalarOutput"/>
  </interface>

</service>



