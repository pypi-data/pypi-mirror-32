<?xml version="1.0"?>
<service name="Green" type="simple" version="1.0" category="DSP">
  
  <input name="X" size="16" type="logic"/>
  <input name="Y" size="16" type="logic"/>
  <input name="Z" size="16" type="logic"/>
  <output name="GreenValue" size="16" type="logic"/>
  
  <interface name="BasicBlock" direction="INOUT" number="1" pipeline="4">
    <data name="X" />
    <data name="Y" />
    <data name="Z" />
    <data name="GreenValue" />
    <protocol label="SetStimuli" position="0" signals="X, Y, Z"/>
    <protocol label="GetResult" position="1" signals="GreenValue"/>
  </interface>

</service>



