<?xml version="1.0"?>
<service name="MatrixVectorMult" type="simple" version="1.0" category="DSP">
  
  <input  name="VectorInput"  size="16" type="logic"/>
  <input  name="MatrixInput"  size="16" type="logic"/>
  <output name="VectorOutput" size="16" type="logic"/>
  
  <interface name="BasicBlock" direction="INOUT" number="1" pipeline="9">
    <data name="VectorInput" serialization="9"/>
    <data name="MatrixInput" serialization="9*9"/>
    <data name="VectorOutput" serialization="1"/>
    <protocol label="SetStimuli" position="0" signals="VectorInput, MatrixInput"/>
    <protocol label="GetTrace" position="1" signals="VectorOutput"/>
  </interface>

</service>



