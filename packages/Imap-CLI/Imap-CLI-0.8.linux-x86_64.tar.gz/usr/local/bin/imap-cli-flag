#!/usr/local/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Imap-CLI==0.8','console_scripts','imap-cli-flag'
__requires__ = 'Imap-CLI==0.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Imap-CLI==0.8', 'console_scripts', 'imap-cli-flag')()
    )
