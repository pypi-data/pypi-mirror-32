#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'exabgp==3.4.26','console_scripts','exabgp'
__requires__ = 'exabgp==3.4.26'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('exabgp==3.4.26', 'console_scripts', 'exabgp')()
    )
